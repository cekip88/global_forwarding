		.shipping_link
			+desktop()
				margin-top 45px
	&-item
		padding-bottom 30px
		+desktop()
			padding-bottom 8px
		&_row
			margin-bottom 15px
		&_title
			white-space initial
	&_link
		text-transform uppercase
