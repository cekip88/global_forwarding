	&_text
		padding 0 32px
		font 16px/20px $y_reg
	&-pilot
		padding 60px 0
		.section,.section_large
			display flex
			flex-direction column
			align-items center
			row-gap 24px
		&_subtitle
			font 30px/36px $t_reg
			text-align center
			+desktop()
				font-size 36px
				line-height 69px
		&_text
			max-width 854px
			font 16px/25px $y_reg
			text-align center
		&_list
			w 100%
			margin-top 36px
			+desktop()
				display flex
				align-items center
		&_item
			display flex
			flex-direction column
			align-items center
		&-score
			align-items center
			+desktop()
				padding 0 18px
			&_title
				margin-bottom 12px
				font 24px/24px $f_bold
			&-mark
				margin-bottom 12px
				display flex
				align-items center
				column-gap 3px
				&_item
