@keyframes logos_scroll
	from
		transform translate(0,0)
	to
		transform translate(-100%,0)
.head
	padding 12px 0 18px
	background-color RGB($white)
	position fixed
	top 0
	left 0
	right 0
	z-index 100
	+desktop()
		padding 0
	&.active
		.head
			&-nav
				&,&-form
					display flex
	&-row
		display flex
		align-items center
		column-gap 20px
		+desktop()
			flex-direction row
			justify-content space-between
			position relative
	&-logo
		max-width 140px
		+desktop()
			max-width initial
		&-cont
			w 100%
			display flex
			align-items center
			justify-content space-between
			+desktop()
				w initial
				flex-direction column
	&-phone
		display none
		+desktop()
			margin-top -2px
			display flex
			align-items center
			column-gap 6px
			font-size 14px
			line-height 28px
			color RGB($light_gray)
			svg
				wh(20px)
	&-burger
		font-size 32px
		color RGB($dark_blue)
		+desktop()
			display none
	&-hidden
		w 100%
		position absolute
		top 100%
		left 0
		+desktop()
			w initial
			position static
	&-nav
		w 100%
		max-height calc(100vh - 150px)
		column-gap 22px
		flex-direction column
		overflow auto
		display none
		+desktop()
			w initial
			display flex
			flex-direction row
			overflow visible
			position static
		&-list,&-sublist
			w 100%
			&.active
				max-height 185px
			&.showed
				max-height initial
			+desktop()
				max-height initial!important
			&-item
				align-items center
		&-list
			border-top 1px solid #d6d6d6
			max-height 0
			overflow hidden
			transition .35s ease
			+desktop()
				padding 40px 20px 50px
				display flex
				flex-direction: column
				align-items: center
				opacity 0
				visibility hidden
				background-color RGB($white)
				position fixed
				top 82px
				left 0
				right 0
			a
				+desktop()
					&:hover
						color RGB($light_blue)
			&_item
				+desktop()
					padding 0 40px
					row-gap 20px
					&:not(:last-child)
						border-right 1px solid #d6d6d6
			&_btn
				padding-left 30px
				background-color #e3e3e3
				border-bottom 1px solid #d6d6d6
				+desktop()
					svg
						display none
		&-sublist
			+desktop()
				w max-content
			&_item
				+desktop()
					margin-bottom 20px
			&_btn
				padding-left 40px
				background-color #f0f0f0
				border-bottom 1px solid #e3e3e3
		&_item,&_btn,&-list_item,&-sublist_item,&-list_btn,&-sublist_btn
			display flex
		&_item,&-list_item,&-sublist_item
			flex-direction column
			position relative
		&_item
			+desktop()
				height 82px
				flex-direction row
				align-items stretch
		&_btn,&-list_btn,&-sublist_btn
			padding 10px 20px
			align-items center
			column-gap 6px
			+desktop()
				padding 0
				background none
				border-bottom none
			svg
				wh(6px)
				transition .35s ease
		&_btn,&-list_btn
			text-transform uppercase
			&.active
				svg
					transform rotate(180deg)
		&_btn
			w 100%
			background-color #d6d6d6
			border-bottom 1px solid #c9c9c9
			+desktop()
				position relative
				&:after
					wh 100%,7px
					content ''
					position absolute
					bottom 0
					left 0
					right 0
					transition .35s ease
		&_item
			+desktop()
				&:hover
					.head-nav_btn
						color RGB($light_blue)
					button.head-nav_btn
						&:after
							background-color RGB($light_blue)
					.head-nav-list
						opacity 1
						visibility visible
	&-nav,&-track
		&-form
			padding 12px 20px
			display none
			justify-content space-between
			background-color RGB($white)
			column-gap 10px
			&.active
				display flex!important
			+desktop()
				wh calc(100% - 370px), 82px
				padding-left 0
				padding-right 100px
				display none!important
				position absolute
				top 0
				right 0
				left initial
			&,&_left
				align-items center
			&_left
				w 100%
				display flex
				position relative
			&_inpt
				w 100%
				padding 12px 13px
				border 1px solid RGB($shipping_blue)
				border-radius 4px
				background-color RGB($white)
				font-size 12px
				line-height 16px
				color RGB($inpt_gray)
			&_btn
				wh 20px
				position absolute
				right 20px
				svg
					wh 100%
			&_close
				wh 20px
				display flex
				align-items center
				justify-content center
				position relative
				&:before,&:after
					wh 15px,2px
					background-color RGB($dark_blue)
					content ''
					position absolute
				&:before
					transform rotate(-45deg)
				&:after
					transform rotate(45deg)
	&-nav
		&-form
			&_close
				display none
				+desktop()
					display flex
	&-track
		&-form
			wh 100%,82px
			display none
			position fixed
			top 0
			left 0
	&_btns
		display flex
		align-items: center
		column-gap 10px
		+desktop()
			column-gap 20px
		.btn
			display none
			margin-right 6px
			+desktop()
				display block
	&-btn
		display flex
		flex-direction column
		align-items center
		gap 4px
		font-size 13px
		white-space nowrap
		text-transform uppercase
		&_search
			display none
			+desktop()
				display flex
		svg
			wh 33px
button.head-nav-list_btn
	+desktop()
		cursor initial
a.head-nav-list_btn
	display none
	+desktop()
		display block
a.head-nav-list_btn + button
	+desktop()
		display none
.footer
	&_inpt
		w 210px
		padding 12px 13px
		border 1px solid RGB($shipping_blue)
		border-radius 4px
		background-color RGB($white)
		font-size 12px
		line-height 16px
		color RGB($inpt_gray)
		&[type="date"]
			padding 11px 13px
	&-bottom
		padding-bottom 24px
		&,&_link
			color RGB($shipping_blue)
		&:before
			wh 100%,1px
			margin 0 auto 30px
			display block
			content ''
			background-color #CACACA
			+desktop()
				w calc(100% - 100px)
				margin-bottom 24px
		&_row
			display flex
			justify-content space-between
			flex-direction column
			row-gap 20px
			+desktop()
				flex-direction row
				align-items center
		&_links
			display flex
			align-items center
			column-gap 20px
			flex-direction column
			row-gap 20px
			+desktop()
				flex-direction row
		&_link
			w 100%
			font-size 14px
			line-height 16px
			+desktop()
				white-space nowrap
	&-copy
		font-size 14px
		line-height 27px


.mark
	wh 25px
	display flex
	align-items center
	justify-content center
	background-color: #DCDCE6
	position relative
	&-row
		display: flex
		align-items: center
		column-gap 2px
	svg
		wh 19px
	&:after
		background-color #00B67A
	svg
		position relative
		z-index 1
	&:after
		height 100%
		content ''
		position absolute
		left 0
		top 0
	&:nth-child(1)
		&:after
			w 100%
.mark-15
	.mark
		&:nth-child(2)
			&:after
				w 50%
.mark-20,.mark-25
	.mark
		&:nth-child(2)
			&:after
				w 100%
.mark-25
	.mark
		&:nth-child(3)
			&:after
				w 50%
.mark-30,.mark-35
	.mark
		&:nth-child(2),&:nth-child(3)
			&:after
				w 100%
.mark-35
	.mark
		&:nth-child(4)
			&:after
				w 50%
.mark-40,.mark-45
	.mark
		&:nth-child(2),&:nth-child(3),&:nth-child(4)
			&:after
				w 100%
.mark-45
	.mark
		&:nth-child(5)
			&:after
				w 50%
.mark-50
	.mark
		&:nth-child(1),&:nth-child(2),&:nth-child(3),&:nth-child(4),&:nth-child(5)
			&:after
				w 100%
.main
	padding-top 82px
	&-banner
		padding 37px 0 0
		background linear-gradient(90deg,rgba(0,0,0,.25),transparent)
		position relative
		+desktop()
			margin-bottom 122px
		.section
			padding 0
			display flex
			flex-direction column
			align-items center
		&:after
			wh 100%
			content ''
			background-color #011E49
			position absolute
			top 0
			z-index -1
		&_row
			w 100%
			padding 0 20px
			display flex
			flex-direction column
			align-items center
			+desktop()
				padding-bottom: 60px
				flex-direction row
		&_col
			text-align center
			+desktop()
				margin-right 80px
				text-align left
		&_title
			font 48px/57.6px $t_bold
			color RGB($white)
		&_subtitle
			margin-top: 20px
			font 24px/28px $t_bold
			color RGB($white)
		&_text
			max-width 530px
			margin-top 38px
			font 24px/28px $y_reg
			color RGB($white)
		&_img
			margin-top 20px
			margin-bottom 60px
			+desktop()
				margin-top 0
		&-form
			margin 0 auto
			padding 42px
			row-gap 30px
			flex-direction column
			background-color RGBA($white)
			box-shadow 0 4px 20px 0 rgba(0,0,0,.06)
			+desktop()
				flex-direction row
				column-gap 40px
				border-radius 8px
				position absolute
				top calc(100% - 58px)
			&,&_col
				display flex
				align-items flex-end
			&_col
				column-gap 10px
			&-label
				w 100%
				display flex
				flex-direction column
				row-gap 6px
				font 12px $f_bold
				+desktop()
					line-height 28px
				&_text
					color RGB($dark_blue)
				&_inpt
					w 100%
					+desktop()
						w 190px
			&_icon
				display none
				+desktop()
					wh 20px
					margin-bottom 17px
					flex-shrink 0
					display block
			&_btn
				w 100%
	&-slider
		padding 60px 0 50px
		display flex
		align-items center
		flex-wrap wrap
		justify-content center
		column-gap 68px
		row-gap 55px
		&_inner
			display flex
			align-items center
			overflow hidden
			+desktop()
				w calc(100% - 200px)
		&_arrow
			display none
			+desktop()
				display block
			svg
				wh 31px,62px
		&_dots
			padding 10px 0
			display flex
			align-items center
			justify-content center
			column-gap 24px
		&_dot
			wh 8px
			border-radius 100%
			background-color #D6D6D6
			&.active
				background-color RGB($dark_blue)
	&-slide
		w 100%
		flex-shrink 0
		display flex
		align-items flex-start
		flex-direction column
		row-gap 20px
		column-gap 45px
		transition .35s ease
		+desktop()
			flex-direction row
		&_left
			flex-shrink 0
			position: relative
			+desktop()
				wh 550px,310px
			.mark-row
				position: absolute
				bottom 20px
				left 20px
			img
				wh 100%
				object-fit cover
			iframe
				wh 100%
		&_right
			padding 5px 0
			display flex
			flex-direction column
			align-items flex-start
			row-gap 25px
		&_title
			font 32px/39px $t_bold
		&_text
			font 20px/26px $y_reg
		&_link
			font 16px/16px $y_reg
			color RGB($dark_blue)
.discover
	&_block
		padding 27px 0
		border-bottom 1px solid #D6D6D6
	&_title
		font 55px/69px $t_reg
	&_subtitle
		font 32px/39px $t_bold
	&_text
		margin-top 27px
		font 16px/20px $y_reg
	&_list
		margin-top 75px
		display flex
		align-items flex-start
		flex-direction column
		row-gap 32px
		+desktop()
			flex-direction row
	&-item
		w 100%
		padding 0 30px 32px
		border-bottom 1px solid #D6D6D6
		&:last-child
			border-bottom none
		+desktop()
			w calc(100% / 3)
			border-right 1px solid #D6D6D6
			border-bottom none
			&:first-child
				padding-left 0
			&:last-child
				padding-right 0
				border-right none
		&_row
			margin-bottom 20px
			display flex
			flex-direction column
			row-gap 20px
			+desktop()
				margin-bottom 0
				flex-direction row
				align-items center
				justify-content space-between
			svg
				wh 80px
				flex-shrink 0
		&_title
			order 1
			font 22px/28px $t_bold
			white-space nowrap
			+desktop()
				order initial
		&_text
			max-width 240px
			margin-bottom 20px
			font 18px/27px $y_reg
			+desktop()
				margin-bottom 0
.shipping
	background-color RGB($shipping_blue)
	.section
