.head
	padding 12px 0 18px
	background-color RGB($white)
	position fixed
	top 0
	left 0
	right 0
	z-index 100
	+desktop()
		padding 0
	&.active
		.head
			&-nav
				&,&-form
					display flex
	&-row
		display flex
		align-items center
		column-gap 20px
		+desktop()
			flex-direction row
			justify-content space-between
			position relative
	&-logo
		max-width 140px
		+desktop()
			max-width initial
		&-cont
			w 100%
			display flex
			align-items center
			justify-content space-between
			+desktop()
				w initial
				flex-direction column
	&-phone
		display none
		+desktop()
			margin-top -2px
			display flex
			align-items center
			column-gap 6px
			font-size 14px
			line-height 28px
			color RGB($light_gray)
			svg
				wh(20px)
	&-burger
		font-size 32px
		color RGB($dark_blue)
		+desktop()
			display none
	&-hidden
		w 100%
		position absolute
		top 100%
		left 0
		+desktop()
			w initial
			position static
	&-nav
		w 100%
		max-height calc(100vh - 150px)
		column-gap 22px
		flex-direction column
		overflow auto
		display none
		+desktop()
			w initial
			display flex
			flex-direction row
			overflow visible
			position static
		&-list,&-sublist
			w 100%
			max-height 0
			overflow hidden
			transition .35s ease
			&.active
				max-height 185px
			&.showed
				max-height initial
			+desktop()
				max-height initial!important
			&-item
				align-items center
		&-list
			border-top 1px solid #d6d6d6
			+desktop()
				padding 40px 20px 50px
				display flex
				justify-content center
				opacity 0
				visibility hidden
				background-color RGB($white)
				position fixed
				top 82px
				left 0
				right 0
			a
				+desktop()
					&:hover
						color RGB($light_blue)
			&_item
				+desktop()
					padding 0 40px
					row-gap 20px
					&:not(:last-child)
						border-right 1px solid #d6d6d6
			&_btn
				padding-left 30px
				background-color #e3e3e3
				border-bottom 1px solid #d6d6d6
				+desktop()
					svg
						display none
		&-sublist
			&_item
				+desktop()
					margin-bottom 20px
			&_btn
				padding-left 40px
				background-color #f0f0f0
				border-bottom 1px solid #e3e3e3
		&_item,&_btn,&-list_item,&-sublist_item,&-list_btn,&-sublist_btn
			display flex
		&_item,&-list_item,&-sublist_item
			flex-direction column
			position relative
		&_item
			+desktop()
				height 82px
				flex-direction row
				align-items stretch
		&_btn,&-list_btn,&-sublist_btn
			padding 10px 20px
			align-items center
			column-gap 6px
			+desktop()
				padding 0
				background none
				border-bottom none
			svg
				wh(6px)
				transition .35s ease
		&_btn,&-list_btn
			text-transform uppercase
			&.active
				svg
					transform rotate(180deg)
		&_btn
			w 100%
			background-color #d6d6d6
			border-bottom 1px solid #c9c9c9
			+desktop()
				position relative
				&:after
					wh 100%,7px
					content ''
					position absolute
					bottom 0
					left 0
					right 0
					transition .35s ease
		&_item
			+desktop()
				&:hover
					.head-nav_btn
						color RGB($light_blue)
					button.head-nav_btn
						&:after
							background-color RGB($light_blue)
					.head-nav-list
						opacity 1
						visibility visible
	&-nav,&-track
		&-form
			padding 12px 20px
			display none
			justify-content space-between
			background-color RGB($white)
			column-gap 10px
			&.active
				display flex!important
			+desktop()
				wh calc(100% - 370px), 82px
				padding-left 0
				padding-right 100px
				display none!important
				position absolute
				top 0
				right 0
				left initial
			&,&_left
				align-items center
			&_left
				w 100%
				display flex
				position relative
			&_inpt
				w 100%
				padding 12px 13px
				border 1px solid RGB($shipping_blue)
				border-radius 4px
				background-color RGB($white)
				font-size 12px
				line-height 16px
				color RGB($inpt_gray)
			&_btn
				wh 20px
				position absolute
				right 20px
				svg
					wh 100%
			&_close
				wh 20px
				display flex
				align-items center
				justify-content center
				position relative
				&:before,&:after
					wh 15px,2px
					background-color RGB($dark_blue)
					content ''
					position absolute
				&:before
					transform rotate(-45deg)
				&:after
					transform rotate(45deg)
	&-nav
		&-form
			&_close
				display none
				+desktop()
					display flex
	&-track
		&-form
			wh 100%,82px
			display none
			position fixed
			top 0
			left 0
	&_btns
		display flex
		column-gap 10px
		+desktop()
			column-gap 20px
		.btn
			display none
			margin-right 6px
			+desktop()
				display block
	&-btn
		display flex
		flex-direction column
		align-items center
		gap 4px
		font-size 13px
		white-space nowrap
		text-transform uppercase
		&_search
			display none
			+desktop()
				display flex
		svg
			wh 33px
button.head-nav-list_btn
	+desktop()
		cursor initial
a.head-nav-list_btn
	display none
	+desktop()
		display block
a.head-nav-list_btn + button
	+desktop()
		display none
.footer
	&_inpt
		w 210px
		padding 12px 13px
		border 1px solid RGB($shipping_blue)
		border-radius 4px
		background-color RGB($white)
		font-size 12px
		line-height 16px
		color RGB($inpt_gray)
		&[type="date"]
			padding 11px 13px
	&-cols
		padding 32px 0
		display flex
		align-items flex-start
		flex-direction column
		column-gap 27px
		row-gap 40px
		+desktop()
			padding-bottom 60px
			flex-direction row
	&-col
		w 210px
		display flex
		flex-direction column
		row-gap 12px
		&_title
			font 12px/17px $t_bold
			color RGB($shipping_blue)
			text-transform uppercase
		&_link
			font-size 14px
			line-height 16px
			color RGB($shipping_blue)
	&-bottom
		padding-bottom 24px
		&,&_link
			color RGB($shipping_blue)
		&:before
			wh 100%,1px
			margin 0 auto 30px
			display block
			content ''
			background-color #CACACA
			+desktop()
				w calc(100% - 100px)
				margin-bottom 24px
		&_row
			display flex
			justify-content space-between
			flex-direction column
			row-gap 20px
			+desktop()
				flex-direction row
				align-items center
		&_links
			display flex
			align-items center
			column-gap 20px
			flex-direction column
			row-gap 20px
			+desktop()
				flex-direction row
		&_link
			w 100%
			font-size 14px
			line-height 16px
			+desktop()
				white-space nowrap
	&-copy
		font-size 14px
		line-height 27px

.main
	padding-top 88px
	&-banner
		padding 37px 0 0
		background linear-gradient(90deg,rgba(0,0,0,.25),transparent)
		position relative
		+desktop()
			margin-bottom 122px
		.section
			padding 0
			display flex
			flex-direction column
			align-items center
		&:after
			wh 100%
			content ''
			background-color #011E49
			position absolute
			top 0
			z-index -1
		&_row
			w 100%
			padding 0 20px
			display flex
			flex-direction column
			align-items center
			+desktop()
				flex-direction row
		&_col
			text-align center
			+desktop()
				margin-right 80px
				text-align left
		&_title
			font 48px/57.6px $t_bold
			color RGB($white)
		&_text
			max-width 530px
			margin-top 38px
			font 24px/28px $y_reg
			color RGB($white)
		&_img
			margin-top 20px
			margin-bottom 60px
			+desktop()
				margin-top 0
		&-form
			margin 0 auto
			padding 42px
			row-gap 30px
			flex-direction column
			background-color RGBA($white)
			box-shadow 0 4px 20px 0 rgba(0,0,0,.06)
			+desktop()
				flex-direction row
				column-gap 40px
				border-radius 8px
				position absolute
				top calc(100% - 58px)
			&,&_col
				display flex
				align-items flex-end
			&_col
				column-gap 10px
			&-label
				w 100%
				display flex
				flex-direction column
				row-gap 6px
				font 12px $f_bold
				+desktop()
					line-height 28px
				&_text
					color RGB($dark_blue)
				&_inpt
					w 100%
					+desktop()
						w 190px
			&_icon
				display none
				+desktop()
					wh 20px
					margin-bottom 17px
					flex-shrink 0
					display block
			&_btn
				w 100%
	&-slider
		padding 60px 0 50px
		display flex
		align-items center
		flex-wrap wrap
		justify-content center
		column-gap 68px
		row-gap 55px
		&_inner
			display flex
			align-items center
			overflow hidden
			+desktop()
				w calc(100% - 200px)
		&_arrow
			display none
			+desktop()
				display block
			svg
				wh 31px,62px
		&_dots
			padding 10px 0
			display flex
			align-items center
			justify-content center
			column-gap 24px
		&_dot
			wh 8px
			border-radius 100%
			background-color #D6D6D6
			&.active
				background-color RGB($dark_blue)
	&-slide
		w 100%
		flex-shrink 0
		display flex
		align-items flex-start
		flex-direction column
		row-gap 20px
		column-gap 45px
		transition .35s ease
		+desktop()
			flex-direction row
		&_left
			flex-shrink 0
			+desktop()
				wh 550px, 310px
			iframe
				wh 100%
		&_right
			padding 5px 0
			display flex
			flex-direction column
			align-items flex-start
			row-gap 25px
		&_title
			font 32px/39px $t_bold
		&_text
			font 20px/26px $y_reg
		&_link
			font 16px/16px $y_reg
			color RGB($dark_blue)
.discover
	&_block
		padding 27px 0
		border-bottom 1px solid #D6D6D6
	&_title
		font 55px/69px $t_reg
	&_subtitle
		font 32px/39px $t_bold
	&_text
		margin-top 27px
		font 16px/20px $y_reg
	&_list
		margin-top 75px
		display flex
		align-items flex-start
		flex-direction column
		row-gap 32px
		+desktop()
			flex-direction row
	&-item
		w 100%
		padding 0 30px 32px
		border-bottom 1px solid #D6D6D6
		&:last-child
			border-bottom none
		+desktop()
			w calc(100% / 3)
			border-right 1px solid #D6D6D6
			border-bottom none
			&:first-child
				padding-left 0
			&:last-child
				padding-right 0
				border-right none
		&_row
			margin-bottom 20px
			display flex
			flex-direction column
			row-gap 20px
			+desktop()
				margin-bottom 0
				flex-direction row
				align-items center
				justify-content space-between
			svg
				wh 80px
				flex-shrink 0
		&_title
			order 1
			font 22px/28px $t_bold
			white-space nowrap
			+desktop()
				order initial
		&_text
			max-width 240px
			margin-bottom 20px
			font 18px/27px $y_reg
			+desktop()
				margin-bottom 0
.shipping
	background-color RGB($shipping_blue)
	.section
		display flex
		flex-direction column
		align-items center
	.footer_inpt
		w 100%
		+desktop()
			w 210px
			margin-right 5px
	.btn
		w 100%
		background-color RGB($light_blue)
		+desktop()
			w initial
	&-form
		w 100%
		padding 33px 0
		display flex
		align-items center
		flex-direction column
		row-gap 10px
		+desktop()
			w initial
			flex-direction row
		&_title
			font-size 22px
			line-height 24px
			color RGB($white)
			+desktop()
				margin-right 11px
	&_bottom
		padding 82px 0 58px
		font-size 18px
		line-height 27px
		text-align center
		color RGB($white)
		+desktop()
			text-align left
	&_list
		margin-top 24px
		img
			margin-top -10px
	&-item
		padding-bottom 30px
		+desktop()
			padding-bottom 8px
		&_row
			margin-bottom 15px
		&_title
			white-space initial
	&_link
		column-gap 4px
		text-transform uppercase
		color RGB($dark_blue)
		+desktop()
			margin-top 45px
			display block
		span,svg
			display inline
			vertical-align middle
		svg
			wh 16px
.news
	padding 25px 0 35px
	background-color RGB($news_gray)
	&,&_link
		color RGB($white)
	&_title
		margin-bottom 4px
		font-size 22px
		line-height 24px
	&_text,&_link
		font-size 14px
		line-height 20px
	&_text
		w 312px
		+desktop()
			margin-right 32px
	&_link
		text-decoration underline
	&-row
		display flex
		align-items center
		justify-content center
		flex-direction column
		text-align center
		+desktop()
			flex-direction row
			text-align left
	&-form
		display flex
		align-items center
		flex-direction column
		column-gap 20px
		+desktop()
			flex-direction row
		&_row
			padding 30px 0
			display flex
			column-gap 10px
			+desktop()
				padding 0
				column-gap 20px
			.footer_inpt
				w 100%
				+desktop()
					w 210px
.trust
	margin 40px 0
	background-color RGB($light_blue)
	color RGB($white)
	text-align center
	+desktop()
		margin-top 124px
		background none
	.section
		padding 55px 20px
		+desktop()
			padding 55px 0
			background-color RGB($light_blue)
	&_title
		font 32px/28px $t_bold
	&_list
		padding 30px 0
		display flex
		flex-direction column
		+desktop()
			padding 62px 0
			flex-direction row
	&-item
		padding 8px 40px 20px
		+desktop()
			border-right 1px solid #D6D6D6
		&:last-child
			border-right none
		&_title
			margin-bottom 20px
			font 28px/28px $t_bold
			+desktop()
				white-space nowrap
		&_text
			font 16px/20px $y_reg
	&_text
		padding 0 32px
		font 16px/20px $y_reg
.fr
	+desktop()
		position relative
	&-top
		padding 2px 0
		background-color #011E49
		text-align center
		&_link
			color RGB($white)
			font 18px/29px $y_reg
	&-middle
		padding 72px 0
		background-image url("img/new_bg.jpg")
		background-size cover
		color RGB($white)
		position relative
		&:after
			wh 100%
			content ''
			background-color #00578E
			opacity .85
			position absolute
			top 0
			left 0
		.section_large
			position relative
			z-index 1
		&_title
			margin-bottom 16px
			font 48px/58px $t_bold
			+desktop()
				max-width 640px
		&_text
			font 16px/25px $y_reg
			+desktop()
				max-width 539px
				font-size 18px
				line-height 28px
			+desktop-large()
				max-width 639px
	&-bottom
		padding 25px 0
		background-color #081320
		.section_large
			display flex
			align-items center
			column-gap 20px
		&-label
			display flex
			align-items center
			column-gap 10px
			font 14px/20px $y_reg
			color RGB($white)
			+desktop()
				w 185px
			+desktop-large()
				w 225px
			svg
				wh 30px
				flex-shrink 0
				+desktop()
					wh 40px
.calc
	padding 0 15px
	+desktop()
		w 560px
		padding 0
		position absolute
		top 53px
		right calc(50% - 600px)
		z-index 1
	+desktop-large()
		right calc(50% - 660px)
	&-form
		padding 24px
		display flex
		flex-direction column
		row-gap 28px
		box-shadow 0 1px 10px 0 rgba(0,0,0,0.08)
		background-color RGB($white)
		border-radius 24px
		&_title
			font 20px/24px $y_bold
			text-align center
		&_label
			w 100%
			display flex
			align-items flex-end
			column-gap 20px
		&-block
			&_row
				display flex
				align-items center
				flex-wrap wrap
				row-gap 18px
				column-gap 15px
				+desktop()
					flex-wrap nowrap
			&_text
				w 100%
				margin-bottom 18px
				display block
				font 18px/24px $y_bold
				color RGB($dark_blue)
			&_parts
				w 100%
				display flex
				align-items flex-end
				column-gap 2px
				+desktop()
					w initial
				.calc-form-inpt
					w 100%
					flex-shrink 1
					overflow hidden
					+desktop()
						max-width 85px
					&_input
						border-radius 0
					&:first-child
						.calc-form-inpt_input
							border-radius 24px 0 0 24px
		&-inpt
			w calc(50% - 8px)
			display flex
			flex-direction column
			align-items flex-start
			row-gap 8px
			position relative
			&_label
				font 14px/20px $y_reg
				color #7C7D81
			&_input
				w 100%
				padding 13px 17px 12px
				border 1px solid #BCBCBC
				border-radius 24px
				font 16px/21px $y_reg
				+desktop()
					padding 13px 15px 12px
		&_select
			w 97px
			flex-shrink 0
			.select
				&_head
					border-radius 0 24px 24px 0
			&.active
				.select
					&_head
						border-bottom-right-radius 0
					&_head,&_body
						border-color #1b4870
		&_submit
			margin 40px auto 0
		.btn
			w max-content
	&_validates_list
		margin-top 20px
		padding-left 20px
		li
			list-style disc
	*.hidden
		display none